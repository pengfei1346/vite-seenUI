{
  "name": "seen-ui-vite",
  "version": "0.0.1",
  "private": true,
  "license": "MIT",
  "workspaces": [
    "packages/components/*",
    "packages/*"
  ],
  "scripts": {
    "website-dev": "vite",
    "website-build": "vite build",
    "test": "jest",
    "bootstrap": "yarn --frozen-lockfile && yarn lerna bootstrap",
    "clean": "rimraf lib",
    "build:all": "yarn clean && yarn build:components && yarn build:theme",
    "build:components": "cross-env NODE_ENV=production yarn rollup -c build/rollup.config.js",
    "build:theme": " rimraf packages/theme-chalk/lib && cross-env NODE_ENV=production gulp build --gulpfile packages/theme-chalk/gulpfile.js && cp-cli packages/theme-chalk/lib lib/theme-chalk && rimraf packages/theme-chalk/lib",
    "deploy": "bash deploy.sh",
    "build:esm-bundle": "rollup -c ./builds/rollup.config.bundle.js",
    "build:esm": "rollup -c ./builds/rollup.config.js"
  },
  "dependencies": {
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.8.4",
    "@iconify/json": "^1.1.248",
    "@iconify/vue": "^2.0.0-rc.1",
    "@rollup/plugin-babel": "^5.2.1",
    "@rollup/plugin-commonjs": "^15.1.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "@rollup/plugin-typescript": "^6.1.0",
    "@tailwindcss/postcss7-compat": "^2.1.2",
    "@tailwindcss/typography": "^0.2.0",
    "@types/jest": "^26.0.10",
    "@vue/compiler-sfc": "^3.0.0",
    "@vue/test-utils": "^2.0.0-beta.3",
    "@vueuse/core": "^4.0.0-rc.10",
    "autoprefixer": "^9",
    "clean-css": "^4.2.3",
    "cp-cli": "^2.0.0",
    "cross-env": "^7.0.2",
    "jest": "^25.1.0",
    "lerna": "^3.22.1",
    "markdown-it": "^11.0.1",
    "markdown-it-prism": "^2.1.2",
    "node-fs-extra": "^0.8.2",
    "postcss": "^7",
    "rollup": "^2.28.2",
    "rollup-plugin-css-only": "^2.1.0",
    "rollup-plugin-esbuild": "^2.5.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.27.3",
    "rollup-plugin-vue": "^6.0.0-beta.11",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat",
    "ts-loader": "^8.0.3",
    "typescript": "^4.0.5",
    "vite": "^1.0.0-rc.8",
    "vite-plugin-vuedoc": "^1.0.0",
    "vue-jest": "5.0.0-alpha.1",
    "vue-router": "^4.0.0-beta.4"
  }
}
