{
  "name": "seen-ui-vite",
  "version": "0.0.1",
  "private": true,
  "license": "MIT",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "website-dev": "vite",
    "website-build": "vite build",
    "test": "jest",
    "bootstrap": "yarn --frozen-lockfile && yarn lerna bootstrap && yarn gen:version",
    "gen:version": "node build/gen-version.js",
    "clean": "rimraf lib",
    "build:all": "yarn clean && yarn build:components && yarn build:theme",
    "build:lib": "cross-env LIBMODE=core webpack --config ./build/webpack.config.js",
    "build:lib-full": "cross-env LIBMODE=full webpack --config ./build/webpack.config.js",
    "build:esm-bundle": "rollup -c ./build/rollup.config.bundle.js",
    "build:esm": "node ./build/bincomp.js",
    "build:theme": " rimraf packages/theme-chalk/lib && cross-env NODE_ENV=production gulp build --gulpfile packages/theme-chalk/gulpfile.js && cp-cli packages/theme-chalk/lib lib/theme-chalk && rimraf packages/theme-chalk/lib",
    "deploy": "bash deploy.sh",
    "build:v1-esm": "cross-env NODE_ENV=production yarn rollup -c build/v2-rollup.config.js",
    "build:v2-esm": "rollup -c build/v3-rollup.config.js"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.11.4",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-typescript": "^7.10.4",
    "@iconify/json": "^1.1.248",
    "@iconify/vue": "^2.0.0-rc.1",
    "@rollup/plugin-babel": "^5.2.1",
    "@rollup/plugin-commonjs": "^15.1.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "@rollup/plugin-typescript": "^6.1.0",
    "@vue/babel-plugin-jsx": "^1.0.0",
    "@vue/compiler-sfc": "^3.0.9",
    "@vue/component-compiler-utils": "^3.2.0",
    "@types/jest": "^26.0.10",
    "@vue/test-utils": "^2.0.0-beta.3",
    "@vueuse/core": "^4.0.0-rc.10",
    "autoprefixer": "^9",
    "babel-jest": "^26.3.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-preset-vue": "^2.0.2",
    "clean-css": "^4.2.3",
    "cp-cli": "^2.0.0",
    "cross-env": "^7.0.2",
    "css-loader": "^4.2.2",
    "css-minimizer-webpack-plugin": "^1.1.5",
    "file-loader": "^6.0.0",
    "file-save": "^0.2.0",
    "highlight.js": "^10.4.1",
    "html-webpack-plugin": "^4.3.0",
    "jest": "^25.1.0",
    "lerna": "^3.22.1",
    "markdown-it": "^11.0.1",
    "markdown-it-prism": "^2.1.2",
    "node-fs-extra": "^0.8.2",
    "mini-css-extract-plugin": "^0.11.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.28.2",
    "rollup-plugin-css-only": "^2.1.0",
    "rollup-plugin-esbuild": "^2.5.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.27.3",
    "rollup-plugin-vue": "^6.0.0-beta.11",
    "sass": "^1.26.10",
    "sass-loader": "^10.0.1",
    "style-loader": "^1.2.1",
    "ts-loader": "^8.0.3",
    "typescript": "^4.0.5",
    "url-loader": "^4.1.0",
    "vite": "^1.0.0-rc.8",
    "vite-plugin-vuedoc": "^1.0.0",
    "vue": "^3.0.0",
    "vue-jest": "5.0.0-alpha.1",
    "vue-loader": "^16.1.2",
    "vue-router": "^4.0.2",
    "vue-template-compiler": "^2.6.12",
    "webpack": "^4.44.1",
    "webpack-bundle-analyzer": "^3.9.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  }
}
